function generate (n) [n delay (generate (n + 1))];

function sum (xs, a, limit) {
    if limit > 0 
        match xs {
            [x xr]: sum (force (xr), a + x, limit - 1)
        }
    else a
};
 
println (sum (generate (10), 100, 5)); // => 160
println (sum (generate (0), 0, 150000)); // => 11249925000   
