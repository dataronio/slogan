var mk_qend = function () { pair (!ignored, []) };

var make_queue = function () {
    let qend = mk_qend () {
        pair (qend, qend)
    }
};

var putq = function (q, v) {
    let qend = mk_qend () {
        set_head (tail (q), v);
        set_tail (tail (q), qend);
        set_tail (q, qend);
    }
};

var getq = function (q) { head (head (q)) };

var delq = function (q) { set_head (q, tail (head (q))) };
